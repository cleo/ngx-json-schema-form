{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sfl": {
      "projectType": "library",
      "root": "projects/sfl",
      "sourceRoot": "projects/sfl/src",
      "prefix": "sfl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/sfl/tsconfig.lib.json",
            "project": "projects/sfl/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sfl/src/test.ts",
            "tsConfig": "projects/sfl/tsconfig.spec.json",
            "karmaConfig": "projects/sfl/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sfl/tsconfig.lib.json",
              "projects/sfl/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "sfl-launcher": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/sfl-launcher",
      "sourceRoot": "projects/sfl-launcher/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sfl-launcher",
            "index": "projects/sfl-launcher/src/index.html",
            "main": "projects/sfl-launcher/src/main.ts",
            "polyfills": "projects/sfl-launcher/src/polyfills.ts",
            "tsConfig": "projects/sfl-launcher/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/sfl-launcher/src/favicon.ico",
              "projects/sfl-launcher/src/assets"
            ],
            "styles": [
              "projects/sfl-launcher/src/styles.scss",
              "node_modules/bootstrap-sass/assets/stylesheets/_bootstrap.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/sfl-launcher/src/environments/environment.ts",
                  "with": "projects/sfl-launcher/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sfl-launcher:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sfl-launcher:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sfl-launcher:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sfl-launcher/src/test.ts",
            "polyfills": "projects/sfl-launcher/src/polyfills.ts",
            "tsConfig": "projects/sfl-launcher/tsconfig.spec.json",
            "karmaConfig": "projects/sfl-launcher/karma.conf.js",
            "assets": [
              "projects/sfl-launcher/src/favicon.ico",
              "projects/sfl-launcher/src/assets"
            ],
            "styles": [
              "projects/sfl-launcher/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sfl-launcher/tsconfig.app.json",
              "projects/sfl-launcher/tsconfig.spec.json",
              "projects/sfl-launcher/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/sfl-launcher/e2e/protractor.conf.js",
            "devServerTarget": "sfl-launcher:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sfl-launcher:serve:production"
            }
          }
        }
      }
    },
    "sfl-validation": {
      "projectType": "library",
      "root": "projects/sfl-validation",
      "sourceRoot": "projects/sfl-validation/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/sfl-validation/tsconfig.lib.json",
            "project": "projects/sfl-validation/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sfl-validation/src/test.ts",
            "tsConfig": "projects/sfl-validation/tsconfig.spec.json",
            "karmaConfig": "projects/sfl-validation/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sfl-validation/tsconfig.lib.json",
              "projects/sfl-validation/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "sfl"
}
