{
  "name": "json-schema-form",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build-jsf-prod": "ng build jsf --configuration=production",
    "build-jsf-validation-prod": "ng build jsf-validation --configuration=production",
    "publish": "npm run publish-jsf && npm run publish-jsf-validation",
    "publish-jsf": "ng build jsf --configuration=production && cd ./dist/jsf && npm publish --access public",
    "publish-jsf-validation": "ng build jsf-validation --configuration=production && cd ./dist/jsf-validation && npm publish --access public",
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-validation": "ng test jsf-validation",
    "lint": "eslint --format unix .",
    "lint-fix": "eslint --format unix . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.12",
    "@angular/common": "13.3.12",
    "@angular/compiler": "13.3.12",
    "@angular/core": "13.3.12",
    "@angular/forms": "13.3.12",
    "@angular/platform-browser": "13.3.12",
    "@angular/platform-browser-dynamic": "13.3.12",
    "@angular/router": "13.3.12",
    "ag-grid-angular": "28.0.0",
    "ag-grid-community": "28.0.0",
    "ajv": "6.12.2",
    "bootstrap-sass": "3.4.1",
    "ng-packagr": "^13.3.1",
    "rxjs": "6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.10",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.10",
    "@angular/compiler-cli": "13.3.12",
    "@angular/language-service": "13.3.12",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.168",
    "@types/node": "16.18.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "codelyzer": "^6.0.0",
